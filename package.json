{
  "name": "tinng-client-web",
  "version": "0.3.0",
  "description": "Web client for tinng service",
  "main": "-",
  "dependencies": {},
  "devDependencies": {
    "extract-text-webpack-plugin": "^2.1.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "start": "NODE_ENV=production pm2 start tinng-client-web.js",
    "stop": "pm2 stop tinng-client-web.js",
    "restart": "pm2 restart tinng-client-web.js",
    "prod-client": "webpack --env.mode=production --config=webpack.config.client.js --progress",
    "prod-server": "webpack --env.mode=production --config=webpack.config.server.js --progress",
    "dev-client": "webpack-dev-server --env.mode=development --config=webpack.config.client.js --debug --hot --devtool inline-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8050 --host 0.0.0.0",
    "dev-server": "webpack --env.mode=development --config=webpack.config.server.js --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lerayne/tinng-client-web.git"
  },
  "author": "Michael Yegorov",
  "license": "MIT",
  "homepage": "https://github.com/lerayne/tinng-client-web"
}
